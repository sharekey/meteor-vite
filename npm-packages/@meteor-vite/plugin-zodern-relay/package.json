{
  "name": "@meteor-vite/plugin-zodern-relay",
  "version": "1.0.6",
  "description": "Vite compatability plugin for zodern:relay - typed Meteor methods and publications",
  "main": "dist/Plugin.js",
  "exports": {
    ".": {
      "require": "./dist/Plugin.js",
      "import": "./dist/Plugin.mjs",
      "types": "./dist/Plugin.d.ts"
    },
    "./stubs/*": [
      "./stubs/*.js"
    ]
  },
  "files": [
    "dist",
    "src",
    "stubs"
